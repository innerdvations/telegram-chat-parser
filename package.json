{
  "name": "telegram-parser",
  "version": "1.0.0",
  "description": "Convert Telegram JSON chat exports into an object",
  "main": "dist/index.js",
  "author": "Benjamin Irvin",
  "license": "MIT",
  "pre-commit": {
    "silent": true,
    "run": [
      "lint",
      "test-min"
    ]
  },
  "scripts": {
    "build": "npx tsc",
    "build-watch": "npx tsc-watch",
    "test": "./node_modules/.bin/ts-mocha",
    "test-min": "./node_modules/.bin/ts-mocha --reporter min",
    "test-watch": "./node_modules/.bin/ts-mocha src/**/*.spec.ts tests/**/*.spec.ts --watch",
    "coverage": "./node_modules/.bin/nyc yarn test-min",
    "lint": "./node_modules/.bin/eslint",
    "dev": "yarn run lint && yarn run coverage",
    "dev-watch": "./node_modules/.bin/nodemon --exec yarn dev",
    "dev-watch-poll": "./node_modules/.bin/nodemon -L --exec yarn dev"
  },
  "devDependencies": {
    "@types/chai": "^4.2.15",
    "@types/chai-arrays": "^2.0.0",
    "@types/expect": "^24.3.0",
    "@types/mocha": "^8.2.1",
    "@types/node": "^14.14.35",
    "@typescript-eslint/eslint-plugin": "^4.18.0",
    "@typescript-eslint/parser": "^4.17.0",
    "chai": "^4.3.4",
    "chai-arrays": "^2.2.0",
    "eslint": "^7.22.0",
    "eslint-config-airbnb": "^18.2.1",
    "eslint-config-airbnb-typescript": "^12.3.1",
    "eslint-plugin-import": "^2.22.1",
    "mocha": "^8.3.2",
    "nodemon": "^2.0.7",
    "nyc": "^15.1.0",
    "pre-commit": "^1.2.2",
    "ts-mocha": "^8.0.0",
    "ts-node": "^9.1.1",
    "tsc-watch": "^4.2.9",
    "typescript": "^4.2.3"
  }
}
